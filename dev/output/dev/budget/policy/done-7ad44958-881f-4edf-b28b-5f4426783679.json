                {
                  "id": "txtContent",
                  "conditionValue": null,
                  "details": {
                    "schemaId": "073d28cc-a2f7-46ad-baca-86d36983f4de",
                    "html": "<div class=\"text-center flex flex-col items-center\">\n    <img class=\"mb-5 mt-5\" src=\"{{assetsUrl}}/successtick.png\">\n    <h2><span class=\"text-main-1\">Welcome</span> to the Budget Insurance family {{name}}!</h2>\n    <h3>You will be covered as of {{coverStartDate}}</h3>\n    <h3>Your policy number is {{policyNumber}}</h3>\n</div>\n<div class=\"flex justify-center\">\n    <hr class=\"mb-5 mt-3 w-1/2\"/>\n</div>\n<p class=\"font-bold text-base\">Here's what you need to do next...</p>"
                  }
                },
                {
                  "id": "txtGroup",
                  "conditionValue": null,
                  "details": {
                    "schemaId": "62b8efc3-0223-4167-b24a-06e2060ad5ca",
                    "isArray": true,
                    "formItems": [
                      {
                        "id": "txtDownloadMobile",
                        "conditionValue": "mobile",
                        "details": {
                          "schemaId": "073d28cc-a2f7-46ad-baca-86d36983f4de",
                          "html": "<div class=\"flex flex-row items-start py-3\">\n    <img class=\"w-12\" alt=\"loginIcon\" src=\"{{cmsUrl}}/api/assets/con-fusion/f9a6c437-ee36-463c-9487-d49e115d3558/phone.svg\" />\n    <span class=\"text-sm pl-4 self-center\"><a href=\"https://onelink.to/dsmt2r\" target=\"_blank\" class=\"text-zodiac-blue font-bold\">Download our app</a> to access your dashboard for self-servicing capabilities.</span>\n</div>"
                        }
                      },
                      {
                        "id": "txtDownloadDesktop",
                        "conditionValue": "desktop",
                        "details": {
                          "schemaId": "073d28cc-a2f7-46ad-baca-86d36983f4de",
                          "html": "<div class=\"flex flex-row items-start py-3\">\n    <img class=\"w-12\" alt=\"loginIcon\" src=\"{{cmsUrl}}/api/assets/con-fusion/f9a6c437-ee36-463c-9487-d49e115d3558/phone.svg\" />\n    <span class=\"text-sm pl-4 self-center\"><span class=\"font-bold text-zodiac-blue\">Download our app</span> to access your dashboard for self-servicing capabilities.</span>\n</div>"
                        }
                      },
                      {
                        "id": "txtSubmit",
                        "conditionValue": "isCollected",
                        "details": {
                          "schemaId": "073d28cc-a2f7-46ad-baca-86d36983f4de",
                          "html": "<div class=\"flex flex-row py-3\">\n    <img class=\"w-12\" alt=\"loginIcon\" src=\"{{cmsUrl}}/api/assets/con-fusion/submit-photos.svg\" />\n    <span class=\"text-sm pl-3 self-center\">Complete your car inspection on <span class=\"text-main-1 font-medium\">our app within 48 hours</span> of the cover start date to ensure that you are covered.</span>\n</div>"
                        }
                      },
                      {
                        "id": "txtNetstar",
                        "conditionValue": "netstar",
                        "details": {
                          "schemaId": "073d28cc-a2f7-46ad-baca-86d36983f4de",
                          "html": "<div class=\"flex flex-row py-3\">\n    <img class=\"w-12\" alt=\"loginIcon\" src=\"{{cmsUrl}}/api/assets/con-fusion/call-agent.svg\" />\n    <span class=\"text-sm pl-3 self-center\">Expect a call from an agent to book your car for a <span class=\"text-main-1 font-medium\">Netstar tracking device installation</span> appointment.</span>\n</div>"
                        }
                      },
                      {
                        "id": "txtOwnTracker",
                        "conditionValue": "ownTracker",
